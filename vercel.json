{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps --no-fund --no-audit",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODE_ENV": "production",
    "NEXT_SKIP_TYPECHECK": "1",
    "TYPESCRIPT_SKIP_ERRORS": "true"
  }
} 